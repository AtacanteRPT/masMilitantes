    function getRandomColor(zona) {
        // console.log('ZONA:', zona);
        var color = '';
        if (zona) {

            var letters = '0123456789ABCDEF';
            color = '#';
            for (var i = 0; i < 6; i++) {
                color += letters[Math.floor(Math.random() * 16)];
            }
        }

        return color;
    }

    function getOpacidad(zona) {
        // console.log('ZONA:', zona);
        var color = 0.0;
        if (zona) {
            color = 0.5
        }

